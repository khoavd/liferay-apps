<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping auto-import="false" default-lazy="false">
	<import class="com.dogoo.sample.animal.service.model.AnimalCategoryEntry" />
	<class name="com.dogoo.sample.animal.service.model.impl.AnimalCategoryEntryImpl" table="DG_ANIMAL_CATE">
		<id access="com.liferay.portal.dao.orm.hibernate.LiferayPropertyAccessor" column="ID" name="animalId" type="long">
			<generator class="assigned" />
		</id>
		<property access="com.liferay.portal.dao.orm.hibernate.LiferayPropertyAccessor" column="NAME" name="name" type="com.liferay.portal.dao.orm.hibernate.StringType" />
	</class>
</hibernate-mapping>